algoritmo "ordem inversa"


var
        numA, numB, troca : inteiro

inicio
escreval ("Informe o primeiro numero numA: ")
leia(numA)
escreval (" informe o segundo numero numB: ")
leia(numB)

troca<-numA

numA<-numB

numB<-troca

escreval (" o valor de numA é: ", numA)
escreval (" o valor de numB é: ", numB)

fimalgoritmo